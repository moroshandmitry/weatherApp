{
  "expo": {
    "name": "weatherApp",
    "slug": "weatherApp",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/weather.png",
    "splash": {
      "image": "./assets/weather.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/weather.png"
      },
      "package": "com.moroshandmitriy.weatherApp"
    },
    "web": {
      "favicon": "./assets/weather.png"
    }
  }
}
